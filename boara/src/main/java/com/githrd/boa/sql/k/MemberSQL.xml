<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mSQL">
	<!-- 로그인 처리  -->
	<select id="login" resultType="int" parameterType="mVO">
		SELECT
			COUNT(*)
		FROM
			member
		WHERE
			id = #{id}
			AND isshow = 'Y'
			AND pw = #{pw}
	</select>





</mapper>